{
  "name": "music-lesson-booking",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run next dev",
    "build": "bun run next build",
    "start": "bun run next start",
    "lint": "biome lint ./src/**/*.{ts,tsx}",
    "lint:w": "biome check --write --unsafe ./src/**/*.{ts,tsx}",
    "db:generate": "drizzle-kit generate:sqlite --config=drizzle.config.dev.ts",
    "db:migrate": "tsx src/lib/migrate.ts",
    "db:push": "drizzle-kit push:sqlite --config=drizzle.config.dev.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.dev.ts",
    "db:generate:prd": "drizzle-kit generate:sqlite --config=drizzle.config.prd.ts",
    "db:push:prd": "wrangler d1 migrations apply music-lesson-booking --local"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "biome": "^0.3.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.36.3",
    "lucide-react": "^0.454.0",
    "moment": "^2.30.1",
    "next": "latest",
    "react": "latest",
    "react-big-calendar": "^1.15.0",
    "react-day-picker": "8.10.1",
    "react-dom": "latest",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "@types/react-big-calendar": "^1.15.0",
    "@types/react-dom": "^18",
    "autoprefixer": "latest",
    "better-sqlite3": "^11.5.0",
    "drizzle-kit": "^0.27.1",
    "postcss": "latest",
    "tailwindcss": "latest",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "wrangler": "^3.84.1"
  }
